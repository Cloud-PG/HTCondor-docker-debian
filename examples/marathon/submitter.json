{
  "id": "/submitter",
  "cpus": 1,
  "mem": 512,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "dscnaf/htcondor-debian",
      "parameters": [
        {
          "key": "net",
          "value": "htcondor"
        }
      ]
    }
  },
  "args": [
    "-s",
    "<MASTER_SUBMITTER>",
    "-u",
    "<USER>",
    "-p",
    "<PASSWORD>"
  ]
}
