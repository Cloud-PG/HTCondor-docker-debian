{
  "id": "/exec",
  "cpus": 1,
  "mem": 512,
  "instances": 2,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "dscnaf/htcondor-debian",
      "parameters": [
        {
          "key": "net",
          "value": "htcondor"
        }
      ]
    }
  },
  "args": [
    "-e",
    "192.168.0.47",    
    "-u",
    "<CONFIG_URL>"

  ]
}
